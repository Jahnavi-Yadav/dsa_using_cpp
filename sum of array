// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int sum_of_arr(int arr[],int n){
    int sum;
    for(int i=0;i<n;i++ ){
        sum=sum+arr[i];
      }
     return sum;
  }
int main() {
  int arr[100];
  int size;
  cout<<"enter the no. of elements in array"<<endl;
  cin>>size;
  cout<<"enter the elements"<<endl;
  for(int i=0;i<size;i++){
      cin>>arr[i];
  }
  int s=sum_of_arr(arr,size);
  cout<<"sum of elements is"<<s<<endl;

    return 0;
}
