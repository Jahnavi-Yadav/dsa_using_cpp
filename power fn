// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int power(int a, int b){
    int ans=1;
    for(int i=1;i<=b;i++){
        ans=ans*a;
    }
    return ans;
}
int main() {
    int a,b;
    cout<<"enter the no."<<endl;
    cin>>a;
    cout<<"enter the power"<<endl;
    cin>>b;
    int an=power(a,b);
    cout<<"Answer is :"<<an;

}
