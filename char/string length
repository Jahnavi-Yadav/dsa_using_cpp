// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int get_length(char name[]){
    int count=0;
    for(int i=0;name[i]!='\0';i++){
        count++;
    }
    return count;
}
int main() {
   char name[20];
   cout<<"enter your name"<<endl;
   cin>>name;
  // name[2]='\0';
   cout<<"your name is "<<name<<endl;
   cout<<"length of your name is "<<get_length(name)<<endl;

    return 0;
}
