// Online C++ compiler to run C++ program online
#include <iostream>
#include <vector>
using namespace std;
struct Node{
    public:
    int data;
    Node* next;
    public:
    Node(int data1, Node* next1){
        data=data1;
        next=next1;
    }
} ;
int main() {
    vector<int> arr={2,3,4,5};
    Node* y=new Node(arr[0], nullptr);
    cout<<"Memory Location "<<y<<endl;
    cout<<"Pointer "<<y->next<<endl;
    cout<<"Value of the node "<<y->data<<endl;
    return 0;
}
